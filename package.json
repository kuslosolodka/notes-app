{
  "name": "notes-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prettify": "prettier --write \"**/*.{json,md,scss,css,html,js,cjs}\"",
    "lint:js": "npx eslint \"src/**/*.js\"",
